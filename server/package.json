{
  "name": "katacode-executable-tutorial",
  "version": "1.0.0",
  "description": "A simple Node.js Express app, including automated testing and linting using Jest and Eslint",
  "main": "index.js",
  "scripts": {
    "start": "node src/index.js",
    "lint": "eslint '**/*.js'",
    "lint:fix": "eslint --fix '**/*.js'",
    "lint-test": "npm run lint; jest --collect-coverage",
    "test": "jest",
    "test-coverage": "jest --collect-coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nwessman/katacoda-scenarios.git"
  },
  "keywords": [
    "Node",
    "Express",
    "Automation",
    "Jest",
    "Eslint"
  ],
  "author": "Anders Nilsson & Niklas Wessman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nwessman/katacoda-scenarios/issues"
  },
  "homepage": "https://github.com/nwessman/katacoda-scenarios#readme",
  "dependencies": {
    "express": "^4.17.1",
    "jest": "^26.6.3",
    "@jest/globals": "^26.6.2",
    "joi": "^17.4.0",
    "supertest": "^6.1.3"
  },
  "devDependencies": {
    "eslint": "^7.23.0"
  }
}
